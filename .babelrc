{
    "presets": [
        [
            "@babel/preset-env",
            {
                "modules": "commonjs",
                "targets": {
                    "esmodules": true
                }
            }
        ]
    ],
    "env": {
        "development": {
            "presets": ["@babel/preset-env"]
        },
        "production": {
            "presets": []
        },
        "test": {
            "presets": [],
            "compact": false,
            "plugins": [
                [
                    "rewire",
                    {
                        "exclude": ["**/*.spec.js"]
                    }
                ],
            ]
        }
    }
}
